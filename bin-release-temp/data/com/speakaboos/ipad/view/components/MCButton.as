package com.speakaboos.ipad.view.components{	import com.greensock.TweenLite;	import com.greensock.plugins.ColorTransformPlugin;	import com.greensock.plugins.TintPlugin;	import com.greensock.plugins.TweenPlugin;		import flash.events.Event;	public class MCButton extends CoreButton	{		public function MCButton(){			super();			TweenPlugin.activate([TintPlugin, ColorTransformPlugin]);		}				override public function set downState(down:Boolean):void{			super.downState = down;			//log("set downState: "+down + ", "+this);			if (down){				TweenLite.to(this, .01, {colorTransform:{tint:0xFFFFFF, tintAmount:0.35}});			}else{				TweenLite.to(this, .01, {colorTransform:{tint:0xFFFFFF, tintAmount:0}});			}		}				override protected function onAddedToStage(e:Event):void{			super.onAddedToStage(e);		}				override protected function onRemovedFromStage(e:Event):void{			super.onRemovedFromStage(e);		}			}}