package com.speakaboos.ipad.view.holders.components{	import com.greensock.TweenLite;	import com.greensock.plugins.ColorTransformPlugin;	import com.greensock.plugins.TintPlugin;	import com.greensock.plugins.TweenPlugin;		import flash.display.MovieClip;	public class MCButton extends CoreButton	{		public function MCButton(pButtonMC:MovieClip){			super(pButtonMC);			TweenPlugin.activate([TintPlugin, ColorTransformPlugin]);		}				override public function set downState(down:Boolean):void{			super.downState = down;			//log("set downState: "+down + ", "+this);			if (mc) {				if (down){					TweenLite.to(mc, .01, {colorTransform:{tint:0xFFFFFF, tintAmount:0.35}});				}else{					TweenLite.to(mc, .01, {colorTransform:{tint:0xFFFFFF, tintAmount:0}});				}			}		}	}}