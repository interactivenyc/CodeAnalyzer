package com.speakaboos.ipad.utils.error{			public class FileErrorStruct	{				private var _errorList:Vector.<String>;		private var _success:Boolean;						public function FileErrorStruct(success:Boolean=true){			_success = success;		}				public function set errorList(errorList:Vector.<String>):void{			_errorList = errorList;		}				public function get success():Boolean{			return _success;		}						public function get errorList():Vector.<String>{			return _errorList;		}						}}