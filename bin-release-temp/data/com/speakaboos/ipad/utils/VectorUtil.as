package com.speakaboos.ipad.utils{	public class VectorUtil	{		public function VectorUtil(){}						public static function toVectorOfStrings(arr:Array):Vector.<String>{						//not the best because AS3 does not fully support templates <T>			//the only way this seems to work is by requiring the type to be a specific Object			//in this case, we are enforcing type <String>						/*				also, assumes the items in arr are strings			*/			var v:Vector.<String> = new Vector.<String>;						for each(var thisItem:String in arr){				v.push(thisItem);			}									return v;					}						}}