package com.speakaboos.ipad.events {	/**	 * @author stevewarren	 * TODO: create a pool for events so they are reused	 */	public class GenericDataEvent {		public var data:*;		public var type:String;				public function GenericDataEvent(pType:String, pData:* = null){			setData(pType, pData);		}		public function setData(pType:String, pData:* = null):void{			type = pType;			data = pData || {};		}				public function reset():void {			type = null;			data = null;		}			}}