package com.speakaboos.ipad.models.data{	import com.speakaboos.ipad.models.data.product.ITunesSubscriptionProduct;	import com.speakaboos.ipad.models.data.product.SpeakaboosSubscriptionProduct;		import flash.utils.Dictionary;	public class MobileSubscriptions	{		private var _iTunesProducts:Dictionary;		private var _speakaboosProducts:Vector.<SpeakaboosSubscriptionProduct>;				public function MobileSubscriptions()		{			_iTunesProducts = new Dictionary();			_speakaboosProducts = new Vector.<SpeakaboosSubscriptionProduct>;		}				final public function setITunesProduct(productID: String, product:ITunesSubscriptionProduct):void{			_iTunesProducts[productID] = product;		}				final public function addSpeakaboosProduct(productID:String, product:SpeakaboosSubscriptionProduct):void{			_speakaboosProducts.push(product);		}						final public function getITunesProduct(productID:String):ITunesSubscriptionProduct{			if(!productID in _iTunesProducts)				return null;						return _iTunesProducts[productID];		}				final public function get speakaboosProducts():Vector.<SpeakaboosSubscriptionProduct>{			return _speakaboosProducts;		}				final public function get iTunesProducts():Dictionary{			return _iTunesProducts;		}				final public function set speakaboosProducts(vect:Vector.<SpeakaboosSubscriptionProduct>):void{			_speakaboosProducts = vect;		}				final public function set iTunesProducts(dict:Dictionary):void{			_iTunesProducts = dict;		}				final public function destroy():void{						trace("destroy");			if(_speakaboosProducts)				_speakaboosProducts.length = 0;						_speakaboosProducts = null;						for (var key:Object in _iTunesProducts) 			{ 				trace(key, _iTunesProducts[key]); 				delete _iTunesProducts[key];			} 						_iTunesProducts = null;								}			}}